{
  "description": "Test fixture for getting non-existent exam (404 error case)",
  "test_case": "get_exam_not_found",
  "request": {
    "method": "GET",
    "path": "/v1/exams/non-existent-exam-id",
    "query_params": {}
  },
  "expected_response": {
    "success": false,
    "message": "Exam not found",
    "error": {
      "code": "NOT_FOUND",
      "message": "Exam not found"
    }
  },
  "validation": {
    "status_code": 404,
    "required_fields": ["success", "message", "error"],
    "data_structure": {
      "error": {
        "required_fields": ["code", "message"],
        "types": {
          "code": "string",
          "message": "string"
        }
      }
    }
  }
}