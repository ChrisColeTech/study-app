{
  "name": "study-app-v3-cdk",
  "version": "0.1.0",
  "description": "AWS CDK infrastructure for Study App V3 Backend",
  "main": "dist/src/study-app-v3.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "clean": "rm -rf dist coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "synth": "cdk synth",
    "deploy": "cdk deploy",
    "deploy:dev": "cdk deploy --profile dev",
    "deploy:staging": "cdk deploy --profile staging",
    "deploy:prod": "cdk deploy --profile prod",
    "diff": "cdk diff",
    "destroy": "cdk destroy"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "20.6.0",
    "@types/aws-lambda": "^8.10.119",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "aws-sdk-client-mock": "^3.0.0",
    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.2"
  },
  "dependencies": {
    "aws-cdk-lib": "2.95.1",
    "constructs": "^10.0.0"
  },
  "keywords": [
    "aws",
    "cdk",
    "infrastructure",
    "study-app",
    "serverless"
  ],
  "author": "Study App Team",
  "license": "MIT"
}